{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Requests Per Second",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [{
        "expr": "sum(rate(http_server_request_duration_seconds_count[1m]))",
        "legendFormat": "RPS"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20 }
        }
      }
    },
    {
      "title": "Response Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_server_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_server_request_duration_seconds_bucket[1m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Error Rate (5xx / total)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [{
        "expr": "sum(rate(http_server_request_duration_seconds_count{http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_request_duration_seconds_count[5m])) * 100",
        "legendFormat": "Error %"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "HTTP Status Codes",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 6, "y": 8 },
      "targets": [{
        "expr": "sum(rate(http_server_request_duration_seconds_count[1m])) by (http_response_status_code)",
        "legendFormat": "{{http_response_status_code}}"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "bars", "fillOpacity": 80 }
        }
      }
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "targets": [{
        "expr": "sum(http_server_active_requests)",
        "legendFormat": "Active"
      }],
      "fieldConfig": {
        "defaults": { "unit": "short" }
      }
    },
    {
      "title": ".NET GC Pause Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "targets": [{
        "expr": "rate(dotnet_gc_pause_time_seconds_total[1m]) * 100",
        "legendFormat": "GC Pause %"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20 },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "title": ".NET Thread Pool",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 16 },
      "targets": [
        {
          "expr": "dotnet_thread_pool_thread_count",
          "legendFormat": "Threads"
        },
        {
          "expr": "dotnet_thread_pool_queue_length",
          "legendFormat": "Queue"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "lineWidth": 2 }
        }
      }
    },
    {
      "title": ".NET Memory (Working Set)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
      "targets": [{
        "expr": "dotnet_process_memory_working_set_bytes",
        "legendFormat": "Working Set"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20 }
        }
      }
    },
    {
      "title": "Redis Memory Usage",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "targets": [{
        "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
        "legendFormat": "Memory %"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0, "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      }
    },
    {
      "title": "PostgreSQL Active Connections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "targets": [{
        "expr": "pg_stat_activity_count",
        "legendFormat": "Active Connections"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20 }
        }
      }
    },
    {
      "title": "Redis Cache Hit Rate",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "targets": [{
        "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
        "legendFormat": "Hit Rate %"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "green", "value": 85 }
            ]
          }
        }
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["dotnet", "api", "performance"],
  "templating": { "list": [] },
  "time": { "from": "now-30m", "to": "now" },
